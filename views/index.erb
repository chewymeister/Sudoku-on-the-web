<!-- "Hello Sudoku!" -->
<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Sudoku Online</title>
  <link rel="stylesheet" href="../css/application.css">
</head>
<body>
  
  <div id="sudoku">
    <% 3.times do |row| %>
      <div class="row">
        <% 3.times do |box| %>
          <div class="box">
            <% 3.times do |cell_row| %>
              <div class="cell-row">
                <% 3.times do |cell| %>
                  <div class="cell">
                    <input type="text" maxlength="1" autocomplete="off">
                  </div>
                <% end %>
              </div>
            <% end %>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>

</body>
</html>